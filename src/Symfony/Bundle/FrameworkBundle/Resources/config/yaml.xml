<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="true" />

        <service id="yaml.parser" class="Symfony\Component\Yaml\Parser" />
        <service id="yaml.transformer.import" class="Symfony\Component\Yaml\Tag\Transformer\ImportTransformer" public="false">
            <tag name="yaml.transformer" priority="10" />
        </service>
        <service id="yaml.transformer.ref" class="Symfony\Component\Yaml\Tag\Transformer\ReferenceTransformer" public="false">
            <tag name="yaml.transformer" priority="0" />
        </service>
    </services>
</container>
